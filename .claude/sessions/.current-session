          # Passwords in various formats
          (r'password["\']?\s*[:[REDACTED]"\']?([^"\'\s,}]+)', 'password'),
          (r'DATABASE_URL=[REDACTED-DATABASE_URL:([...',] 'database_url'),
          # API keys and tokens
          (r'api[_-]?key["\']?\s*[:[REDACTED]"\']?([^"\'\s,}]+)', 'api_key'),
