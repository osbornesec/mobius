    "lucide-react": "^0.468.0",
    "@monaco-editor/react": "^4.6.0",
    "prismjs": "^1.29.0",
    "@prismjs/plugin-line-numbers": "^1.5.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.8.1",
    "chart.js": "^4.4.6",
    "react-chartjs-2": "^5.2.0",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^11.12.0",
    "cmdk": "^1.0.4"
